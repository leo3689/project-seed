<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yupaits.auth.mapper.FilterChainMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yupaits.auth.entity.FilterChain">
    <result column="id" property="id" />
    <result column="created_at" property="createdAt" />
    <result column="created_by" property="createdBy" />
    <result column="updated_at" property="updatedAt" />
    <result column="updated_by" property="updatedBy" />
    <result column="deleted" property="deleted" />
        <result column="url" property="url" />
        <result column="filter" property="filter" />
        <result column="sort_code" property="sortCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        created_at,
        created_by,
        updated_at,
        updated_by,
        deleted,
        url, filter, sort_code
    </sql>

</mapper>
